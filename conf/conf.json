{
  "GENERAL": {
    "HOSTNAME": "srv-nc-test1",
    "WORK_FOLDER": "c:\backup",
    "LOCAL_BACKUP": "c:\backup\local",
    "HOME_FOLDER": "c:\backup\nc-backup-py",
    "LOG_FOLDER": "c:\backup\nc-backup-py\log\ncbackup.log"
    },
  "FILESET":{
    "ACTION": "execute",
    "NAME": "filesbackup",
    "EXECUTABLE": "",
    "EXECUTE_WITH": "python",
    "EXECUTABLE": "",
    "PARAMETERS": {
      "FILESET_INCLUDE": "/etc /var/www/py/nc-backup-py/ /var/spool/cron",
      "FILESET_EXCLUDE": "/etc/yum.d/ /etc/yum.conf /etc/yum.repos.d/ /etc/grub.d"
    }
  },
  "COMPRESSION": {
    "ACTION": "execute",
    "NAME": "compression",
    "EXECUTABLE": "",
    "EXECUTE_WITH": "",
    "PARAMETERS": {
      "OBJECTIVES": "/opt/backup/files",
      "DESTINATION": "/opt/backup/compressed",
      "REMOVE_OBJECTIVES": "True"
    }
  },
  "ENCRYPTION": {
    "ACTION": "execute",
    "NAME": "encryption",
    "PARAMETERS": {
      "KEY_FILE": "/var/www/py/nc-backup-py/conf/key_file",
      "FILE_SIZE": "5M",
      "OBJECTIVES": "/opt/backup/compressed",
      "DESTINATION":"/opt/backup/encrypted",
      "REMOVE_OBJECTIVES": "True"
    }
  },
  "STORAGE": {
    "ACTION": "execute",
    "NAME": "storage",
    "PARAMETERS":{
      "OBJECTIVES": "/opt/backup/encrypted",
      "DESTINATION":"local",
      "__options": "local,s3,ssh,oss",
      "OBJECTIVES": "/opt/backup/encrypted",
      "REMOVE_OBJECTIVES": "True"
    }
  }
}
