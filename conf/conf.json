{
  "QA":{
    "ACTION": "load",
    "FROM": "qa",
    "FILENAME": "qa",
    "CLASS": "QA"
  },
  "GENERAL": {
    "HOSTNAME": "srv-nc-py27-vbox-vagrant-db1",
    "WORK_FOLDER": "/opt/backup",
    "LOCAL_BACKUP": "/opt/backup/local",
    "HOME_FOLDER": "/vagrant/nc-backup-py",
    "LOG_FOLDER": "/var/log/nc-backup-py/nc-backup-py.log"
    },
  "FILESET":{
    "ACTION": "execute",
    "NAME": "filesbackup",
    "EXECUTE_WITH": "python",
    "EXECUTABLE": "",
    "PARAMETERS": {
      "FILESET_INCLUDE": "/etc /vagrant",
      "FILESET_EXCLUDE": "",
      "TAR_COMMAND": "sudo /bin/tar czCf /"
    }
  },
  "DBSBACKUP": {
    "ACTION": "execute",
    "NAME": "mysql_backup",
    "EXECUTABLE": "mysql_dump.py",
    "EXECUTE_WITH": "python",
    "PARAMETERS": {
      "CREDENTIAL_PATH": "/vagrant/nc-backup-py/conf/mysql.credentials",
      "CONF_PATH": "/etc/my.cnf",
      "DATA_DIR": "/var/lib/mysql",
      "MY_INSTANCES": "3306",
      "BINLOG_PATH": "/var/lib/mysql",
      "BINLOG_FILE_PREFIX": "mysql-bin",
      "DESTINATION": "/opt/backup",
      "TAR_COMMAND": "sudo /bin/tar czf",
      "MYSQL_DUMP_BINARY": "/bin/mysqldump",
      "MYSQL_BINARY": "/bin/mysql",
      "PREFIX_FOLDER": "mydump",
      "EXCLUDE_DB": "",
      "EXCLUDE_TABLE": "mysql.user mysql.func mysql.event"
    }
  },
  "COMPRESSION": {
    "ACTION": "execute",
    "NAME": "compression",
    "EXECUTABLE": "",
    "EXECUTE_WITH": "",
    "PARAMETERS": {
      "TAR_COMMAND": "sudo /bin/tar czCf /",
      "OBJECTIVES": "/opt/backup/files /opt/backup/mydump",
      "DESTINATION": "/opt/backup/compressed",
      "REMOVE_OBJECTIVES": "True"
    }
  },
  "ENCRYPTION": {
    "ACTION": "execute",
    "NAME": "encryption",
    "PARAMETERS": {
      "KEY_FILE": "/vagrant/nc-backup-py/conf/key_file",
      "FILE_SIZE": "4000",
      "OBJECTIVES": "/opt/backup/compressed",
      "DESTINATION":"/opt/backup/encrypted",
      "REMOVE_OBJECTIVES": "True"
    }
  },
  "SIZE":{
    "ACTION": "load",
    "FROM": "tools",
    "FILENAME": "size_calculation",
    "CLASS": "SizeCalculation",
    "PARAMETERS": {
      "OBJECTIVES": "/opt/backup/encrypted"
    }
  },
  "STORAGE": {
    "ACTION": "execute",
    "NAME": "storage",
    "PARAMETERS":{
      "OBJECTIVES": "/opt/backup/encrypted",
      "DESTINATION":"s3",
      "BUCKET_NAME": "cncbackup",
      "UPLOAD_COMMAND": "aws s3 cp",
      "REMOVE_OBJECTIVES": "False"
    }
  },
  "STORAGE_OSS": {
    "ACTION": "execute",
    "NAME": "storage",
    "PARAMETERS":{
      "OBJECTIVES": "/opt/backup/encrypted",
      "DESTINATION":"oss",
      "BUCKET_NAME": "cncbackup",
      "ALIYUN_CREDENTIALS": "/etc/.alioss.conf",
      "REMOVE_OBJECTIVES": "True"
    }
  },
  "CLEANUP":{
    "BUCKET": "cncbackup",
    "PREFIX": "srv-nc-backup-py-test1/",
    "KEEP": {
      "DAILY": {"NUMBER": "7"},
      "WEEKLY": {"NUMBER": "2", "DAY": "1"},
      "MONTHLY": {"NUMBER": "2", "DAY": "1"}
    }
  }
}