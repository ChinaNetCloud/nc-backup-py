{
  "GENERAL": {
    "HOSTNAME": "srv-nc-py27-vbox-vagrant-db1",
    "WORK_FOLDER": "/opt/backup",
    "LOCAL_BACKUP": "/opt/backup/local",
    "HOME_FOLDER": "/vagrant/nc-backup-py",
    "LOG_FOLDER": "/var/log/nc-backup-py/nc-backup-py.log"
  },
  "FILESET":{
    "ACTION": "execute",
    "NAME": "filesbackup"
  },
  "MYSQL": {
    "ACTION": "execute",
    "NAME": "mysql_backup",
    "EXECUTABLE": "mysql_dump.py",
    "EXECUTE_WITH": "python",
    "PARAMETERS": {
      "CREDENTIAL_PATH": "/etc/nc-backup-py/mysql.credentials",
      "DATA_DIR": "/var/lib/mysql",
      "BINLOG_PATH": "/var/lib/mysql",
      "BINLOG_FILE_PREFIX": "mysql-bin",
      "MYSQL_DUMP_BINARY": "/usr/bin/mysqldump",
      "MYSQL_BINARY": "/usr/bin/mysql",
      "PREFIX_FOLDER": "mydump",
      "EXCLUDE_DB": "brt",
      "BINLOG_DAYS": "3"
    }
  },
  "COMPRESSION": {
    "ACTION": "execute",
    "NAME": "compression"
  },
  "ENCRYPTION": {
    "ACTION": "execute",
    "NAME": "encryption"
  },
  "SIZE":{
    "ACTION": "load",
    "FROM": "tools",
    "FILENAME": "size_calculation",
    "CLASS": "SizeCalculation",
    "PARAMETERS": {
      "TARGETS": "/opt/backup/encrypted"
    }
  },
  "STORAGE": {
    "ACTION": "execute",
    "NAME": "storage",
    "PARAMETERS":{
      "DESTINATION":"s3",
      "BUCKET_NAME": "cncbackup"
    }
  }
}