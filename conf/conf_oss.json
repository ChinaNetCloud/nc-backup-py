{
  "GENERAL": {
    "HOSTNAME": "srv-nc-py27-vbox-vagrant-db1",
    "WORK_FOLDER": "/opt/backup",
    "LOCAL_BACKUP": "/opt/backup/local",
    "HOME_FOLDER": "/vagrant/nc-backup-py",
    "LOG_FOLDER": "/var/log/nc-backup-py/nc-backup-py.log"
  },
  "FILESET":{
    "ACTION": "execute",
    "NAME": "filesbackup"
  },
  "COMPRESSION": {
    "ACTION": "execute",
    "NAME": "compression"
  },
  "ENCRYPTION": {
    "ACTION": "execute",
    "NAME": "encryption"
  },
  "SIZE":{
    "ACTION": "load",
    "FROM": "tools",
    "FILENAME": "size_calculation",
    "CLASS": "SizeCalculation",
    "PARAMETERS": {
      "TARGETS": "/opt/backup/encrypted"
    }
  },
  "STORAGE_OSS": {
    "ACTION": "execute",
    "NAME": "storage",
    "PARAMETERS":{
      "OBJECTIVES": "/opt/backup/encrypted",
      "DESTINATION":"oss",
      "BUCKET_NAME": "ihehadatabackup",
      "ALIYUN_CREDENTIALS": "/etc/.alioss.conf",
      "REMOVE_OBJECTIVES": "True"
    }
  }
}
